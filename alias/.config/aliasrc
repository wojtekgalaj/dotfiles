alias cleanup='git branch --merged | grep -v "\*" | xargs -n 1 git branch -d'
alias cleanup_origin='git branch -r --merged | grep -v "(^\*|master|main)" | cut -d/ -f2- | xargs -n 1 git push --delete origin'
